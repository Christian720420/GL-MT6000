config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi'
	option band '2g'
	option channel '6'
	option htmode 'HE20'
	option country 'US'
	option cell_density '0'
	option txpower '20'
	option legacy_rates '0'
	option noscan '1'
	option beacon_int '100'
	option diversity '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/18000000.wifi+1'
	option band '5g'
	option channel 'auto'
	option htmode 'HE160'
	option cell_density '0'
	option country 'US'
	option legacy_rates '0'
	option noscan '1'
	option beacon_int '100'
	option diversity '1'
	option txpower '23'

config wifi-iface 'wifinet0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Tomorrow'\''s Land'
	option encryption 'sae-mixed'
	option key 'Perez1718'
	option network 'lan'
	option he_su_beamformer '1'
	option he_su_beamformee '1'
	option he_mu_beamformer '1'
	option ht_capab '[TX-STBC][RX-STBC1][SHORT-GI-20][SHORT-GI-40][MAX-AMSDU-7935]'
	option he_twt_responder '1'
	option he_bss_color '10'
	option airtime_fairness '1'
	option aql_enable '1'
	option aql_txq_limit '2000'
	option he_triggered_su_beamforming '1'
	option he_triggered_mu_beamforming '1'
	option he_mu_mimo_grouping '1'
	option he_mu_edca '1'
	option fils_discovery '1'
	option he_bcn_rate 'MCS9'
	option vht_bcn_rate 'MCS7'
	option hostapd_options 'config_file=/etc/hostapd/hostapd-phy0.conf'
	option ocv '0'
	option disassoc_low_ack '1'
	option short_preamble '1'
	option dtim_period '2'
	option max_num_sta '32'
	option multicast_to_unicast '1'
	option isolate '1'

config wifi-iface 'wifinet1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Tomorrow'\''s Land'
	option encryption 'sae-mixed'
	option key 'Perez1718'
	option ocv '2'
	option network 'lan'
	option he_su_beamformer '1'
	option he_su_beamformee '1'
	option he_mu_beamformer '1'
	option vht_capab '[MAX-MPDU-11454][SHORT-GI-80][SHORT-GI-160][TX-STBC-2BY1][RX-STBC-1][RX-ANTENNA-PATTERN][TX-ANTENNA-PATTERN][RXLDPC][MAX-A-MPDU-LEN-EXP7]'
	option he_twt_responder '1'
	option he_bss_color '20'
	option airtime_fairness '1'
	option aql_enable '1'
	option aql_txq_limit '2000'
	option he_triggered_su_beamforming '1'
	option he_triggered_mu_beamforming '1'
	option he_mu_mimo_grouping '1'
	option he_mu_edca '1'
	option fils_discovery '1'
	option he_bcn_rate 'MCS9'
	option vht_bcn_rate 'MCS7'
	option hostapd_options 'config_file=/etc/hostapd/hostapd-phy1.conf'
	option disassoc_low_ack '1'
	option short_preamble '1'
	option dtim_period '2'
	option max_num_sta '32'
	option multicast_to_unicast '1'
	option isolate '1'